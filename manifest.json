{
  "name": "Extension name",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Your description",
  "icons": {
    "16": "Link-your-icon",
    "128": "Link-your-icon"
  },
  "default_locale": "vi",
  "browser_action": {
    "default_icon": {
      "19": "Link-your-icon",
      "38": "Link-your-icon"
    },
    "default_title": "Your title"
  },
  "content_scripts": [
    {
      "matches": [
        "*://Link-web-need-change/*"
      ],
      "css": [
        "css/css.css"
      ],
      "js": [
        "js/js.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "css/css.css"
  ],
  "web_accessible_resources": [
    "js/js.js"
  ],
  "permissions": [
    "cookies",
    "*://Link-web-need-change/*"
  ]
}
